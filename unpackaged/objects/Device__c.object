    <validationRules>
        <fullName>DeviceUseableOnlyByOneUser</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT($User.BypassVR__c),
Is_used__c &gt;1)</errorConditionFormula>
        <errorMessage>This device is already used by another person.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OtherTypeFill</fullName>
        <active>true</active>
        <description>Fill the &quot;Other type&quot; field only if Material type equals Other.</description>
        <errorConditionFormula>AND(ISBLANK( OtherType__c ), ISPICKVAL( MaterialType__c ,&apos;Other&apos;), (NOT( $User.BypassVR__c )))</errorConditionFormula>
        <errorMessage>You have to specify the material type on &quot;Other type&quot;.</errorMessage>
    </validationRules>
